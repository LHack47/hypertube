{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "axios": "^0.16.1",
    "browser-cookies": "^1.1.0",
    "prop-types": "^15.5.9",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "react-scripts": "0.9.5"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "plugins": [
      "react",
      "import",
      "jsx-a11y"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": [
        0
      ],
      "import/extensions": [
        0
      ]
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
