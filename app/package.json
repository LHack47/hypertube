{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "browser-cookies": "^1.1.0",
    "material-ui": "^0.19.1",
    "prop-types": "^15.5.9",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.2",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-auth": "0.0.5-beta5",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "react-scripts": "1.0.11"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true
    },
    "plugins": [
      "react",
      "import",
      "jsx-a11y"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": [
        0
      ],
      "import/extensions": [
        0
      ]
    }
  },
  "scripts": {
    "geni18n": "better-npm-run geni18n",
    "buildi18n": "rm -rf ./src/i18n/messages && BABEL_ENV=i18n ./node_modules/babel-cli/bin/babel.js --quiet src > /dev/null",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "betterScripts": {
    "geni18n": {
      "command": "node ./bin/i18n-generate",
      "env": {
        "DEBUG": "app:*"
      }
    }
  }
}
